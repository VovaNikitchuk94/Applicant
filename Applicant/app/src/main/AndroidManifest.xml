<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.example.vova.applicant">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activities.TopLevelActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".activities.UniversitiesListActivity"/>
        <activity android:name=".activities.AboutUniversityActivity"/>
        <activity android:name=".activities.CitiesListActivity"/>
        <activity android:name=".activities.SpecialtiesListActivity"/>
        <activity android:name=".activities.ApplicationListActivity"/>
        <activity android:name=".activities.DetailUniversListActivity"/>
        <activity android:name=".activities.TimeFormListActivity"/>
        <activity android:name=".activities.DetailApplicantPagerActivity"/>
    </application>

</manifest>