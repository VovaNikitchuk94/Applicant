<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.example.vova.applicant">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".TopLevelActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".UniversitiesListActivity"/>
        <activity android:name=".AboutUniversityActivity"/>
        <activity android:name=".CitiesListActivity"/>
        <activity android:name=".FullTimeFormListActivity"/>
        <activity android:name=".ExternalFormListActivity"/>
        <activity android:name=".UniversityPageActivity"/>
        <activity android:name=".ExternalFormPageActivity"/>
        <activity android:name=".FullTimeFormPageActivity"/>
        <activity android:name=".BachelorListActivity"/>
        <activity android:name=".SpecialistListActivity"/>
        <activity android:name=".MasterListActivity"/>
    </application>

</manifest>