<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:layout_width="match_parent"
              android:layout_height="match_parent"
              android:orientation="horizontal"
              android:padding="16dp">

    <TextView
        android:id="@+id/textViewSequenceNumberApplicantInfo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>

    <TextView
        android:id="@+id/textViewNameApplicantsApplicantInfo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:paddingStart="16dp"/>

    <TextView
        android:id="@+id/textViewCompetitionScoresApplicantInfo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:paddingStart="16dp"/>

    <TextView
        android:id="@+id/textViewBDOScoreApplicantsApplicantInfo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:paddingStart="16dp"/>

    <TextView
        android:id="@+id/textViewZNOScoreApplicantsApplicantInfo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:paddingStart="16dp"/>
</LinearLayout>